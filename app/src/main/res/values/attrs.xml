<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="translationTextKey" format="string" />
    <attr name="translationLabelKey" format="string" />
    <attr name="translationHintKey" format="string" />
    <attr name="translationInfoKey" format="string" />
    <attr name="isFocused" format="boolean"/>
    <attr name="icon" format="reference"/>
    <attr name="translationSecondaryTextKey" format="string"/>
    <attr name="mask" format="string"/>
    <attr name="inputHeight" format="dimension"/>

    <declare-styleable name="MSDTextView">
        <attr name="translationTextKey"/>
        <attr name="isUnderlined" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MSDLinksTextView">
        <attr name="translationTextKey"/>
    </declare-styleable>

    <declare-styleable name="MSDPhoneInput">
        <attr name="phoneMask" format="string"/>
        <attr name="countryImage" format="reference"/>
        <attr name="errorTranslationTextKey" format="string"/>
    </declare-styleable>

    <declare-styleable name="MSDCodeInput">
        <attr name="disabledBackground" format="reference"/>
        <attr name="errorBackground" format="reference"/>
        <attr name="disabledTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="MSDTextViewLoader">
        <attr name="translationTextKey"/>
        <attr name="android:text"/>
        <attr name="android:enabled"/>
    </declare-styleable>

    <declare-styleable name="MSDTextViewLoaderFullwidth">
        <attr name="translationTextKey"/>
        <attr name="android:text"/>
        <attr name="android:enabled"/>
    </declare-styleable>

    <declare-styleable name="MSDTextViewLoaderDoubleText">
        <attr name="translationPrimaryTextKey" format="string" />
        <attr name="translationSecondaryTextKey"/>
        <attr name="android:text"/>
        <attr name="android:enabled"/>
    </declare-styleable>

    <declare-styleable name="MSDTextViewSecondary">
        <attr name="translationTextKey"/>
    </declare-styleable>

    <declare-styleable name="MSDImageView">
        <attr name="android:enabled"/>
        <attr name="isActivated" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MSDTextViewIcon">
        <attr name="translationTextKey"/>
        <attr name="icon"/>
    </declare-styleable>

    <declare-styleable name="MSDLabelTextView">
        <attr name="translationLabelKey"/>
    </declare-styleable>

    <declare-styleable name="MSDLabelIconTextView">
        <attr name="icon"/>
        <attr name="translationLabelKey"/>
    </declare-styleable>

    <declare-styleable name="MSDBaseEditText">
        <attr name="translationHintKey"/>
    </declare-styleable>

    <declare-styleable name="MSDEditText">
        <attr name="translationHintKey"/>
        <attr name="android:imeOptions"/>
        <attr name="android:inputType" />
        <attr name="android:textAllCaps" />
        <attr name="prefix" format="string"/>
        <attr name="prefixVisible" format="boolean"/>
        <attr name="android:lines"/>
        <attr name="isFocused"/>
        <attr name="android:maxLength"/>
        <attr name="android:digits"/>
    </declare-styleable>

    <declare-styleable name="MSDLabelEditText">
        <attr name="translationHintKey"/>
        <attr name="translationLabelKey"/>
        <attr name="translationSecondaryTextKey"/>
        <attr name="android:imeOptions"/>
        <attr name="android:inputType" />
        <attr name="labelColor" format="color" />
        <attr name="android:textAllCaps" />
        <attr name="android:lines"/>
        <attr name="isFocused"/>
        <attr name="android:maxLength"/>
        <attr name="android:digits"/>
    </declare-styleable>

    <declare-styleable name="MSDPolicyInput">
        <attr name="translationHintKey"/>
        <attr name="translationLabelKey"/>
        <attr name="translationInfoKey"/>
        <attr name="translationSecondaryTextKey"/>
        <attr name="android:imeOptions"/>
        <attr name="android:inputType" />
        <attr name="android:textAllCaps" />
        <attr name="android:lines"/>
        <attr name="isFocused"/>
        <attr name="android:maxLength"/>
        <attr name="android:digits"/>
    </declare-styleable>

    <declare-styleable name="MSDIconEditText">
        <attr name="translationHintKey"/>
        <attr name="mask"/>
        <attr name="icon"/>
        <attr name="android:imeOptions"/>
        <attr name="android:inputType" />
        <attr name="android:textAllCaps" />
        <attr name="android:lines"/>
        <attr name="isFocused"/>
        <attr name="android:maxLength"/>
        <attr name="android:digits"/>
    </declare-styleable>

    <declare-styleable name="MSDLabelIconEditText">
        <attr name="translationHintKey"/>
        <attr name="translationLabelKey"/>
        <attr name="mask"/>
        <attr name="icon"/>
        <attr name="android:imeOptions"/>
        <attr name="android:inputType" />
        <attr name="android:textAllCaps" />
        <attr name="android:lines"/>
        <attr name="isFocused"/>
        <attr name="android:maxLength"/>
        <attr name="android:digits"/>
    </declare-styleable>

    <declare-styleable name="MSDMaskedLabelEditText">
        <attr name="translationHintKey"/>
        <attr name="translationLabelKey"/>
        <attr name="mask"/>
        <attr name="android:imeOptions"/>
        <attr name="android:inputType" />
        <attr name="android:textAllCaps" />
        <attr name="android:lines"/>
        <attr name="isFocused"/>
        <attr name="android:maxLength"/>
        <attr name="hintAlwaysVisible" format="boolean"/>
        <attr name="titleTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="MSDTextInputLayout">
        <attr name="translationHintKey"/>
        <attr name="android:imeOptions"/>
        <attr name="android:inputType" />
        <attr name="android:textAllCaps" />
        <attr name="android:lines"/>
        <attr name="isFocused"/>
        <attr name="android:maxLength"/>
        <attr name="android:digits"/>
        <attr name="decimalLimit" format="string"/>
        <attr name="focusedTranslationHintKey" format="string"/>
        <attr name="android:text"/>
        <attr name="unfocusOnDone" format="boolean"/>
        <attr name="boxStrokeWidth" format="dimension"/>
        <attr name="boxStrokeWidthFocused" format="dimension"/>
        <attr name="enabled" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MSDGenderSelector">
        <attr name="translationLabelKey"/>
    </declare-styleable>

    <declare-styleable name="MSDYesNoSelector">
        <attr name="translationLabelKey"/>
    </declare-styleable>

    <declare-styleable name="MSDSearchInput">
        <attr name="translationHintKey"/>
        <attr name="android:enabled"/>
        <attr name="inputHeight"/>
        <attr name="icon"/>
    </declare-styleable>

    <declare-styleable name="MSDSearchAddressInput">
        <attr name="translationHintKey"/>
        <attr name="android:enabled"/>
        <attr name="inputHeight"/>
        <attr name="icon"/>
    </declare-styleable>

    <declare-styleable name="MSDRecyclerView">
        <attr name="horizontalDecorationGap" format="dimension"/>
        <attr name="verticalDecorationGap" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MSDRoundedFrameLayout">
        <attr name="cornerRadius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="MSDProductPriceView">
        <attr name="priceType" format="enum">
            <enum name="fixed" value="0"/>
            <enum name="unfixed" value="1"/>
            <enum name="purchased" value="2"/>
            <enum name="included" value="3"/>
        </attr>
    </declare-styleable>
</resources>
